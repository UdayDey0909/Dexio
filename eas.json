{
   "cli": {
      "version": ">= 7.0.0"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         }
      },
      "preview": {
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         },
         "env": {
            "NODE_ENV": "preview"
         }
      },
      "production": {
         "distribution": "store",
         "android": {
            "buildType": "app-bundle"
         },
         "env": {
            "NODE_ENV": "production"
         }
      }
   },
   "submit": {
      "production": {}
   }
}
